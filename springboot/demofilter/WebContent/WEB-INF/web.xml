<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>demofilter</display-name>
  
  <servlet>
    <servlet-name>Hello1</servlet-name>
    <servlet-class>com.demo.Hello1</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Hello1</servlet-name>
    <url-pattern>/hello1.do</url-pattern>
  </servlet-mapping>
  
   <servlet>
    <servlet-name>Hello2</servlet-name>
    <servlet-class>com.demo.Hello2</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Hello2</servlet-name>
    <url-pattern>/hello2.do</url-pattern>
  </servlet-mapping>
  
  <filter>
    <filter-name>F1</filter-name>
    <filter-class>com.demo.F1</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>F1</filter-name>
    <url-pattern>*.do</url-pattern>
  </filter-mapping>
  
  
</web-app>